.popupView{
    width: 100vw;
    height: 100vh;
    z-index: 97;
    display: flex;
    flex-direction: column;
    position: fixed;
    top: 0;
    overflow: hidden;
    background-color: rgba(0, 0, 0, 0.5);
}
.popupView .content{
    padding: 40rpx;
    background-color: #f5f5f5;
    border-radius: 12rpx 12rpx 0 0;
    overflow: hidden;
    position: fixed;
    width: 100vw;
    box-sizing: border-box;
    bottom: 0;
    z-index: 99;
}
